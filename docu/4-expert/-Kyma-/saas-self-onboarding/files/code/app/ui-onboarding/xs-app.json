{
  "welcomeFile": "/index.html",
  "authenticationMethod": "route",
  "routes": [
    {
      "source": "^/catalog/(.*)$",
      "target": "/catalog/$1",
      "destination": "obd-api",
      "authenticationType": "ias"
    },
    {
      "source": "^/v2/catalog/(.*)$",
      "target": "/v2/catalog/$1",
      "destination": "obd-api",
      "authenticationType": "ias"
    },
    {
      "source": "^(.*)$",
      "service": "html5-apps-repo-rt",
      "authenticationType": "ias"
    }
  ]
}